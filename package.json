{
  "devDependencies": {
    "assemblyscript": "AssemblyScript/assemblyscript",
    "loop": "^3.1.3",
    "typescript": "^3.4.1"
  },
  "scripts": {
    "asbuild:untouched": "asc src/assembly/doubleArray.ts -b src/build/untouched.wasm -t src/build/untouched.wat --sourceMap --validate --debug",
    "asbuild:optimized": "asc src/assembly/doubleArray.ts -b src/build/optimized.wasm -t src/build/optimized.wat --sourceMap --validate --optimize",
    "asbuild:untouched2": "asc src/assembly/add.ts -b src/build/untouched2.wasm -t src/build/untouched2.wat --sourceMap --validate --debug",
    "asbuild:optimized2": "asc src/assembly/add.ts -b src/build/optimized2.wasm -t src/build/optimized2.wat --sourceMap --validate --optimize",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized && npm run asbuild:untouched2  && npm run asbuild:optimized2",
    "watch": "watch 'npm run asbuild' ./src/assembly",
    "start": "npm run watch & parcel index.html",
    "l": "loop asbuild:untouched"
  },
  "dependencies": {
    "watch": "^1.0.2"
  }
}
